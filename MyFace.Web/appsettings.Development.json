{
  "Uploads": {
    "MalwareScanner": {
        "Enabled": false,
      "EngineName": "ClamAV",
      "Command": "/usr/bin/clamscan",
      "Arguments": "--no-summary --infected",
      "TimeoutSeconds": 20,
      "WorkingDirectory": "/tmp",
      "BlockOnError": false,
      "FailOpenWhenUnavailable": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
