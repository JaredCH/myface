{
  "Uploads": {
    "MalwareScanner": {
        "Enabled": false,
      "EngineName": "ClamAV",
      "Command": "/usr/bin/clamscan",
      "Arguments": "--no-summary --infected",
      "TimeoutSeconds": 20,
      "WorkingDirectory": "/tmp",
      "BlockOnError": false,
      "FailOpenWhenUnavailable": true
    }
  },
  "CustomHtmlSettings": {
    "MaxFileSizeBytes": 512000,
    "MaxOutputBytes": 512000,
    "MaxNodeCount": 1000,
    "AllowDataImages": true,
    "AllowExternalUrls": false,
    "AllowSvg": false,
    "AllowedTags": [
      "div", "span", "p", "br", "hr", "h1", "h2", "h3", "h4", "h5", "h6",
      "ul", "ol", "li", "strong", "b", "em", "i", "u", "blockquote",
      "pre", "code", "table", "thead", "tbody", "tr", "th", "td", "img", "a"
    ]
  },
  "CustomHtmlStorage": {
    "RootDirectory": "user-html",
    "RequestPathPrefix": "/u",
    "FileName": "profile.html",
    "BackupFolderName": "versions",
    "MaxVersionsPerUser": 5,
    "MaxWriteBytes": 512000,
    "EnableSecureDelete": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
